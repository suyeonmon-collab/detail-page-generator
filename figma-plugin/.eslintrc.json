{
  "env": {
    "browser": true,
    "es2017": true
  },
  "globals": {
    "figma": "readonly",
    "parent": "readonly"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-console": "off",
    "no-alert": "off",
    "prefer-const": "off",
    "no-var": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  }
}
