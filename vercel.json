{
  "buildCommand": null,
  "outputDirectory": ".",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/admin", "destination": "/admin/index.html" },
    { "source": "/pages/(.*)", "destination": "/pages/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30,
      "maxRequestBodySize": "10mb"
    }
  }
}

